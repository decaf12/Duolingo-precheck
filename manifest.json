{
    "manifest_version": 2,
    "name": "Duolingo Precheck",
    "version": "1.0",
  
    "description": "Pre-submission checks for correctness.",
    
    "content_scripts": [
      {
        "matches": ["*://*.duolingo.com/*"],
        "js": ["src/injectScripts.js"]
      }
    ],

    "web_accessible_resources": [
      "src/*.js"
    ]
  }
